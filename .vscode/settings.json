// Place your settings in this file to overwrite default and user settings.
{
    "cSpell.ignorePaths": [
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        "package.json",
        "**/cspell.json",
        "**/cSpell.json",
        "**/.cspell.json",
        "dictionaries",
        ".vscode",
        "typings"
    ]
    ,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "dist": true
    },
    "cSpell.words": [
        "EPUB",
        "backend",
        "readwrite",
        "toolchain"
    ],
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pub",
        "python",
        "restructuredtext",
        "rust",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.ignoreRegExpList": ["email"]
}